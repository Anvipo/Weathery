                android:layout_height="wrap_content"
                android:contentDescription="@string/humidity_value"
                android:text="@string/humidity_value"
                tools:ignore="UnusedIds" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/border"
            android:baselineAligned="false"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/border"
                android:orientation="vertical">

                <TextView
                    style="@style/MyMaterialSubtitle1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:contentDescription="@string/precipitation_type"
                    android:text="@string/precipitation_type" />

                <TextView
                    android:id="@+id/precipitation_type_value_YW_CW_TV"
                    style="@style/MyMaterialBody1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:contentDescription="@string/precipitation_type_value"
                    android:text="@string/precipitation_type_value"
                    tools:ignore="UnusedIds" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/border"
                android:orientation="vertical">

                <TextView
                    style="@style/MyMaterialSubtitle1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:contentDescription="@string/precipitation_strength"
                    android:text="@string/precipitation_strength" />

                <TextView
                    android:id="@+id/precipitation_strength_value_YW_CW_TV"
                    style="@style/MyMaterialBody1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:contentDescription="@string/precipitation_strength_value"
                    android:text="@string/precipitation_strength_value"
                    tools:ignore="UnusedIds" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/border"
            android:orientation="vertical">

            <TextView
                style="@style/MyMaterialSubtitle1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="@string/cloudiness"
                android:text="@string/cloudiness" />

            <TextView
                android:id="@+id/cloudiness_value_YW_CW_TV"
                style="@style/MyMaterialBody1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="@string/cloudiness_value"
                android:text="@string/cloudiness_value"
                tools:ignore="UnusedIds" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/border"
            android:orientation="vertical">

            <TextView
                style="@style/MyMaterialSubtitle1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="@string/daytime"
                android:text="@string/daytime" />

            <TextView
                android:id="@+id/daytime_value_YW_CW_TV"
                style="@style/MyMaterialBody1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="@string/daytime_value"
                android:text="@string/daytime_value"
                tools:ignore="UnusedIds" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/border"
            android:orientation="vertical">

            <TextView
                style="@style/MyMaterialSubtitle1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="@string/is_there_polar"
                android:text="@string/is_there_polar" />

            <TextView
                android:id="@+id/polar_value_YW_CW_TV"
                style="@style/MyMaterialBody1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="@string/is_there_polar_value"
                android:text="@string/is_there_polar_value"
                tools:ignore="UnusedIds" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/border"
            android:orientation="vertical">

            <TextView
                style="@style/MyMaterialSubtitle1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="@string/season"
                android:text="@string/season" />

            <TextView
                android:id="@+id/season_value_YW_CW_TV"
                style="@style/MyMaterialBody1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="@string/season_value"
                android:text="@string/season_value"
                tools:ignore="UnusedIds" />
        </LinearLayout>
    </LinearLayout>
</com.google.android.material.card.MaterialCardView>