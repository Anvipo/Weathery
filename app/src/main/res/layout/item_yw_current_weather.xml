<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/MyMaterialCardView"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/item_yw_current_weather"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:ignore="UnusedIds">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/border"
            android:orientation="vertical">

            <TextView
                style="@style/MyMaterialSubtitle1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="@string/observation_time"
                android:text="@string/observation_time" />

            <TextView
                android:id="@+id/observation_unix_time_value_YW_CW_TV"
                style="@style/MyMaterialBody1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="@string/observation_time_value"
                android:text="@string/observation_time_value" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/border"
            android:baselineAligned="false"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/border"
                android:orientation="vertical">

                <TextView
                    style="@style/MyMaterialSubtitle1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:contentDescription="@string/temperature"
                    android:text="@string/temperature" />

                <TextView
                    android:id="@+id/temperature_value_YW_CW_TV"
                    style="@style/MyMaterialBody1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:contentDescription="@string/temperature_value"
                    android:text="@string/temperature_value"
                    tools:ignore="UnusedIds" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/border"
                android:orientation="vertical">

                <TextView
                    style="@style/MyMaterialSubtitle1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:contentDescription="@string/feels_like_temperature"
                    android:text="@string/feels_like_temperature" />

                <TextView
                    android:id="@+id/feels_like_temperature_value_YW_CW_TV"
                    style="@style/MyMaterialBody1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:contentDescription="@string/feels_like_temperature_value"
                    android:text="@string/feels_like_temperature_value"
                    tools:ignore="UnusedIds" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/border"
            android:orientation="vertical">

            <TextView
                style="@style/MyMaterialSubtitle1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="@string/condition"
                android:text="@string/condition" />

            <TextView
                android:id="@+id/condition_value_YW_CW_TV"
                style="@style/MyMaterialBody1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentDescription="@string/condition_value"
                android:text="@string/condition_value"
                tools:ignore="UnusedIds" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/border"
            android:baselineAligned="false"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/border"
                android:orientation="vertical">

                <TextView
                    style="@style/MyMaterialSubtitle1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:contentDescription="@string/wind_speed"
                    android:text="@string/wind_speed" />

                <TextView
                    android:id="@+id/wind_speed_value_YW_CW_TV"
                    style="@style/MyMaterialBody1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:contentDescription="@string/wind_speed_value"
                    android:text="@string/wind_speed_value"
                    tools:ignore="UnusedIds" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/border"
                android:orientation="vertical">

                <TextView
                    style="@style/MyMaterialSubtitle1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:contentDescription="@string/wind_gusts_speed"
                    android:text="@string/wind_gusts_speed" />

                <TextView
                    android:id="@+id/wind_gusts_speed_value_YW_CW_TV"
                    style="@style/MyMaterialBody1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:contentDescription="@string/wind_gusts_speed_value"
                    android:text="@string/wind_gusts_speed_value"
                    tools:ignore="UnusedIds" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/border"
                android:orientation="vertical">

                <TextView
                    style="@style/MyMaterialSubtitle1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:contentDescription="@string/wind_direction"
                    android:text="@string/wind_direction" />

                <TextView
                    android:id="@+id/wind_direction_value_YW_CW_TV"
                    style="@style/MyMaterialBody1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:contentDescription="@string/wind_direction_value"
                    android:text="@string/wind_direction_value"
                    tools:ignore="UnusedIds" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/border"
            android:baselineAligned="false"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/border"
                android:orientation="vertical">

                <TextView
                    style="@style/MyMaterialSubtitle1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:contentDescription="@string/atmospheric_pressure_mm_hg"
                    android:text="@string/atmospheric_pressure" />

                <TextView
                    android:id="@+id/atmospheric_pressure_in_mm_hg_value_YW_CW_TV"
                    style="@style/MyMaterialBody1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:contentDescription="@string/atmospheric_pressure_mm_hg_value"
                    android:text="@string/atmospheric_pressure_value"
                    tools:ignore="UnusedIds" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/border"
                android:orientation="vertical">

                <TextView
                    style="@style/MyMaterialSubtitle1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:contentDescription="@string/atmospheric_pressure_hPa"
                    android:text="@string/atmospheric_pressure" />

                <TextView
                    android:id="@+id/atmospheric_pressure_in_hPa_value_YW_CW_TV"
